.activity
{
	background: #fff;
	min-height: 50px;
	position: relative;
	zoom: 1;
	-ms-word-break: break-all;
     word-break: break-all;

     /* Non standard for webkit */
     word-break: break-word;

	-webkit-hyphens: auto;
   	-moz-hyphens: auto;
    hyphens: auto;
}
.activity:before,.activity:after
{
	content: " ";
	display: table;
}
.activity:after
{
	clear: both;
}
.activity:last-child
{
	border-bottom: none;
}
.activity .readable
{
	margin: 0 2px 0 2px;
	padding: 0 2px;
	zoom: 1;
}
.activity .readable:before,.activity .readable:after
{
	content: " ";
	display: table;
}
.activity .readable:after
{
	clear: both;
}
.activity .readable .user
{
	color: #aeb9bd;
	float: left;
	font-size: 14px;
	font-weight: bold;
	margin-bottom: 4px;
	width: 100%;

}
.activity .readable .content
{
	color: #797d7f;
	float: left;
	font-size: 14px;
	word-wrap: break-word;
}
.activity .readable .content .image-preview,.activity .readable .content .youtube-preview
{
	background: #f3f8f9;
	border-radius: 4px;
	box-shadow: 0px 1px 5px 0px #ccc;
	float: left;
	padding: 10px;
}
.activity .readable .content .image-preview,.activity .readable .content .image-preview>*,.activity .readable .content .youtube-preview,.activity .readable .content .youtube-preview>*
{
	max-width: 100%;
}
.activity .readable .content .image-preview a,.activity .readable .content .youtube-preview a
{
	display: block;
}
.activity .readable .content .image-preview .name,.activity .readable .content .youtube-preview .name
{
	color: #878d8f;
	font-weight: bold;
	margin-top: 5px;
}
.activity .readable .content img.image-embed
{
	height: auto;
	max-width: 100%;
	width: 100%;
}

.activity .readable .content .youtube-preview
{
	height: auto;
	max-width: 100%;
	width: 100%;
}
.activity .readable .content .color-preview
{
	border: 1px solid gray;
	border-radius: 3px;
	display: inline-block;
	height: 15px;
	margin: 0 5px;
	vertical-align: bottom;
	width: 15px;
}
.activity .readable .content .mention
{
	background-color: #bcd3e8;
	border: 1px solid #bbb;
	border-radius: 5px;
	color: #000;
	padding: 2px 3px;
}
.activity .readable .content .emoticon-embed
{
	vertical-align: bottom;
}
.activity .readable .content .emoticon-embed.small
{
	height: 20px;
	width: 20px;
}
.activity .readable .content .emoticon-embed.big
{
	height: 40px;
	width: 40px;
}
.activity .posted_at
{
	background: #f3f8f9;
	border-radius: 4px;
	color: #bdc6ca;
	float: right;
	font-size: 11px;
	padding: 4px;
	text-align: right;
}
.activity .avatar
{
	display: block;
	float: left;
	height: 30px;
	width: 30px;
}
.activity.current_user
{
	background-color: #f5f5f5;
}
.activity.current_user .posted_at
{
	background: #bcd3e8;
	color: #6f7477;
}
.activity.mentioned_user
{
	background-color: #e2ecf5;
}
.activity.mentioned_user .posted_at
{
	background: #bcd3e8;
	color: #6f7477;
}
.activity.connect
{
	background-color: #c4df9b;
}
.activity.disconnect
{
	background-color: #fbaf5c;
}
.activity.connect,.activity.disconnect
{
	min-height: 0;
	opacity: 0.5;
	padding: 3px;
	padding-left: 10px;
	padding-right: 10px;
}
.activity.connect .posted_at,.activity.disconnect .posted_at
{
	line-height: normal;
}
.activity.connect .avatar,.activity.connect .user,.activity.disconnect .avatar,.activity.disconnect .user
{
	display: none;
}
.activity.connect .user,.activity.connect .content,.activity.disconnect .user,.activity.disconnect .content
{
	float: none;
}
.pagination
{
	font-size: 14px;
	line-height: 2em;
	padding: 10px 0;
	text-align: center;
}
.pagination i
{
	margin-right: 7px;
	vertical-align: middle;
}
.activity,.pagination
{
	border-bottom: 1px solid #dee6e8;
	padding: 10px;
}
