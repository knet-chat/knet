.profileImage {
	display: block;  
	margin-left : auto;
	margin-right : auto;
	padding: 0;
	-webkit-border-radius: 50%;
	border-radius: 50%;
	border: 0;
	max-width : 500px;
	max-height : 500px;
	width: 100%;
	height: 100%;      
}

.darkink {
	  height: auto!important;
	  letter-spacing: -0.01em;
	  background-color: #fff;
	  -webkit-margin-end: 0px;
	  -webkit-margin-start: 0px;
	  -webkit-margin-after: 0.83em;
	  -webkit-margin-before: 0.83em;
	  display: block;
	  box-sizing: border-box;
	  margin-left : auto;
	  margin-right : auto;
	  margin-top: 20px;
	  margin-bottom: 10px;
	  line-height: 1.30em;
	  font-weight: 300;
	  font-size: 30px;
	  text-shadow: none;
	  color: #333333;
	  text-align : center;
	  max-width : 100%;
	  max-height : 100%;	
}
